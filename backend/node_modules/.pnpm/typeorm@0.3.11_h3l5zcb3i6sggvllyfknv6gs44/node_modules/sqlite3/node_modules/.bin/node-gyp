#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Chris/Documents/_Projects/flashcards_online/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/d/Chris/Documents/_Projects/flashcards_online/backend/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../node-gyp@8.4.1/node_modules/node-gyp/bin/node-gyp.js" "$@"
else
  exec node  "$basedir/../../../../../node-gyp@8.4.1/node_modules/node-gyp/bin/node-gyp.js" "$@"
fi
